{
  "name": "angular-cbc-example-app",
  "version": "1.0.0",
  "description": "The purpose of this module is to provide a quick way to start a new Angular 1.x application, using ES6 modules and its awesome syntax! You will use some conventions in order to maximize the result with less effort!",
  "main": "webpack.config.js",
  "scripts": {
    "directives": "./node_modules/.bin/angular-cbc compile-directives",
    "start": "concurrently \"npm run directives\" \"webpack-dev-server --progress --color\"",
    "bundle": "./node_modules/.bin/webpack -p"
  },
  "author": "",
  "license": "ISC",
  "bin": "./node_modules/.bin/angular-cbc",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-preset-env": "^1.4.0",
    "babel-preset-es2015": "^6.24.1",
    "cheerio": "^0.22.0",
    "concurrently": "^3.4.0",
    "css-loader": "^0.28.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "glob": "^7.1.1",
    "html-webpack-plugin": "^2.28.0",
    "node-sass": "^4.5.2",
    "node-watch": "^0.5.2",
    "open-browser-webpack-plugin": "0.0.5",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "webpack-dev-server": "^2.4.2",
    "webpack": "^2.4.1",
  },
  "dependencies": {
    "angular-ui-router": "^0.4.2",
    "angular": "^1.6.4"
  }
}
